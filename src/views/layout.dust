  <!DOCTYPE html>
  <html>
    <head>
      <!--Let browser know website is optimized for mobile-->
      <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
      <!--Import Google Icon Font-->
      <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
      <!--Import materialize.css-->
      <link type="text/css" rel="stylesheet" href="/materialize-css/dist/css/materialize.min.css"  media="screen,projection"/>

      <link rel="stylesheet" href="/css/style.css">
      <title>Mapa Rural RS</title>
      <link rel="shortcut icon" href="imagens/favicon.ico" type="imagens/icon" />
    </head>
    <body class="main">
        <nav class="teal" role="navigation">
          <div class="nav-wrapper container"><a id="logo-container" href="/" class="brand-logo"><h5>MapaRuralRS</h5><icon href="imagens/favicon.ico" /></a>
            <ul class="right hide-on-med-and-down">
              <li><a href="/">Inicio</a></li>
            </ul>
            <ul class="center brand-logo hide-on-med-and-down">
              <li style="font-size: 80%;" id="txtCidadeLogo"></li>
            </ul>
            <ul id="nav-mobile" class="sidenav">
              <li><a href="#"></a></li>
            </ul>
            <a href="#" data-target="nav-mobile" class="sidenav-trigger"><i class="material-icons">menu</i></a>
          </div>
        </nav>
    
        {+body /}
        
        <div class="preloader-background">
          <div class="preloader-wrapper big active">
            <div class="spinner-layer spinner-green-only">
              <div class="circle-clipper left">
                <div class="circle"></div>
              </div>
              <div class="gap-patch">
                <div class="circle"></div>
              </div>
              <div class="circle-clipper right">
                <div class="circle"></div>
              </div>
            </div>
          </div>
        </div>

        <script src='http://maps.googleapis.com/maps/api/js?v=3.6&sensor=false&libraries=geometry'></script>
        
                
        <script src="https://code.jquery.com/jquery-2.1.1.min.js"></script>
        <script src="/js/script.js"></script>
        <!--JavaScript at end of body for optimized loading-->
        <script type="text/javascript" src="/materialize-css/dist/js/materialize.min.js"></script>
    </body>
  </html>
        